const JSONConfig = require('./config.json');
const instance = require('./axiosInstance');

test("<%= testCaseDescription %>", async () => {
    try {
        <% if (method === 'get') { %>

            // request which is going to be sent in the test case
            instance.<%= method %>('<%= path %>', {
              params : {...JSONConfig.queryParams},
              headers : {...JSONConfig.headers}
            }).then(res => {
              console.log(res);
            }).catch(console.error)
            
            <%} else { %>
            
            // request which is going to be sent in the test case
            instance.<%= method %>('<%= path %>' , {...JSONConfig.requestBody} , {
              params : {...JSONConfig.queryParams},
              headers : {...JSONConfig.headers}
            }).then(res => {
              console.log(res);
            }).catch(console.error)
            
            <% } %>
             
    } catch (e) {
        expect(e).toMatch('error');
    }
});